class Water {
    pour(incr){
        this.vol = this.vol + incr;
    }
}

class Density {
}

var water = Water();
var density = Density();

water.density = density;
water.density.val = 1;

print water;
print water.density;
print water.density.val;

water.vol = 1;
water.pour(1);
print water.vol;

print "Test dynamically assigning a function to an instance";
fun dump() {
    print "dump() called";
}
water.dump = dump;
water.dump();

